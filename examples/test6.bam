def std: import "std/std.bam";
def String: std.Types.String;

force def main: fn[] -> i32 {
    def s: new String = String.from("Hello ");
    
    s.append("World!");


    if (!(class {def a: i32; def b: 100;}.a == i32)) {
        String.println(s);
        return i32(1);
    } else {
        def lol: new i32 = i32(1); 

        while (*lol < i32(100)) {
            String.println(s);

            lol = *lol + i32(1);
        }

        return i32(0);
    }
};
