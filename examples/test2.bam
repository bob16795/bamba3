def mainBuilderBuilder: class {
    def buildMainBuilder: fn[T] -> Class {
        return class {
            def printf: extern fn[] -> Void;
    
            def buildMain: fn[value: T] -> (fn[] -> T) {
                return fn[] -> i32 {
                    printf("%s\n", (T)(value));
    
                    return i32(0);
                };
            };
        };
    };
};

force def main: mainBuilderBuilder.buildMainBuilder(*u8[]).buildMain("Hello World!");
